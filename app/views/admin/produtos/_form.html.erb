<%= form_for [:admin,@produto], :builder => MNE::TwitterBootstrapFormBuilder::FormBuilder, :html => { :class => "form-horizontal" } do |f| %>
							
<%= f.text_field :nome %>
<%= f.text_field :desc %>
<%= f.text_field :valor %>
			
<div class="form-actions">
	<%= f.submit nil, :class => 'btn btn-primary' %>
	<%= link_to t('.cancel', :default => t("helpers.links.cancel")),
	admin_produtos_path, :class => 'btn' %>
</div>
<% end %>
